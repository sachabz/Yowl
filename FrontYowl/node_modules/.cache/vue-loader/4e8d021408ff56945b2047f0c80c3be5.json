lass=\"\n              mb-5\n              p-3\n              w-80\n              focus:border-purple-700\n              rounded\n              border-2\n              outline-none\n            \"\n            autocomplete=\"off\"\n            placeholder=\"Date\"\n          />\n           <input\n            v-model=\"id_post\"\n            name=\"id_post\"\n            class=\"\n              mb-5\n              p-3\n              w-80\n              focus:border-purple-700\n              rounded\n              border-2\n              outline-none\n            \"\n            autocomplete=\"off\"\n            placeholder=\"Id du post\"\n          />\n          <button\n            class=\"\n              bg-red-500\n              hover:bg-red-400\n              text-white\n              font-bold\n              p-2\n              rounded\n              w-80\n            \"\n            id=\"poster\"\n            type=\"submit\"\n            @click=\"poster\"\n          >\n            <span>Poster le commentaire</span>\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nconst axios = require(\"axios\");\n\nexport default {\n  name: \"NewCommentInput\",\n  components: {},\n  data: () => {\n    return {\n      id_post: \"\",\n      auteur: \"\",\n      date: \"\",\n      contenu: \"\",\n    };\n  },\n  methods: {\n    poster(event) {\n      event.preventDefault();\n      axios\n        .post(\"http://localhost:8000/api/comment\", {\n          id_post: this.id_post,\n          auteur: this.auteur,\n          date: this.date,\n          contenu: this.contenu,\n        })\n        .then((response) => {\n          console.log(response.data);\n          var result = response.data;\n          if (result != null) {\n            alert(\"Post bien créé!\");\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        }),\n        axios\n          .get(\"http://localhost:8000/api/comment\", {})\n          .then((response) => {\n            console.log(response.data);\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n    },\n  },\n};\n</script>\n"]}]}